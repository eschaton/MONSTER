! Korjattu 6.5.1992 ?
! 17.6.1992 $(LINK), $(COMPILE), + ja ALL
! 25.6.1992 ALLOC.PAS
! 30.6.1992 VERSION.PAS, MONSTER_E.HLB

LINK = LINK
COMPILE = PASCAL /CHECK=ALL

ALL : MON.EXE, MONSTER_WHO.EXE, MONSTER_DUMP.EXE, MONSTER_REBUILD.EXE,-
MONSTER_E.HLB
	$ WRITE SYS$OUTPUT "Done"
MONSTER_E.HLB : MONSTER_E.HLP
	$ LIBRARIAN/CREATE/HELP MONSTER_E MONSTER_E
MON.EXE : MON.OBJ,GUTS.OBJ,INTERPRETER.OBJ,KEYS.OBJ,PRIVUSERS.OBJ,QUEUE.OBJ,-
PARSER.OBJ,CLI.OBJ,GLOBAL.OBJ,DATABASE.OBJ,CUSTOM.OBJ,ALLOC.OBJ,VERSION.OBJ
	$(LINK) MON,GUTS,INTERPRETER,KEYS,PRIVUSERS,QUEUE,PARSER,CLI,GLOBAL,-
DATABASE,CUSTOM,ALLOC,VERSION	! Linkkaus
MONSTER_WHO.EXE : MONSTER_WHO.OBJ, DATABASE.OBJ, GUTS.OBJ, GLOBAL.OBJ,-
PRIVUSERS.OBJ, PARSER.OBJ, KEYS.OBJ
	$(LINK) MONSTER_WHO, DATABASE, GUTS, GLOBAL, PRIVUSERS, PARSER
MONSTER_DUMP.EXE : MONSTER_DUMP.OBJ, DATABASE.OBJ, GUTS.OBJ, GLOBAL.OBJ,-
PRIVUSERS.OBJ, PARSER.OBJ, VERSION.OBJ
	$(LINK) MONSTER_DUMP, DATABASE, GUTS, GLOBAL, PRIVUSERS, PARSER, -
	VERSION
MONSTER_REBUILD.EXE : MONSTER_REBUILD.OBJ, DATABASE.OBJ, GUTS.OBJ, GLOBAL.OBJ,-
PRIVUSERS.OBJ, PARSER.OBJ, ALLOC.OBJ, KEYS.OBJ, VERSION.OBJ
	$(LINK) MONSTER_REBUILD, DATABASE, GUTS, GLOBAL, PRIVUSERS, PARSER, -
	ALLOC, KEYS, VERSION
MONSTER_WHO.OBJ : MONSTER_WHO.PAS, DATABASE.PEN, GUTS.PEN, GLOBAL.PEN, -
PRIVUSERS.PEN, PARSER.PEN
	$(COMPILE) MONSTER_WHO
MONSTER_DUMP.OBJ : MONSTER_DUMP.PAS, DATABASE.PEN, GUTS.PEN, GLOBAL.PEN, -
PRIVUSERS.PEN, PARSER.PEN
	$(COMPILE) MONSTER_DUMP
MONSTER_REBUILD.OBJ : MONSTER_REBUILD.PAS, DATABASE.PEN, GUTS.PEN, GLOBAL.PEN, -
PRIVUSERS.PEN, PARSER.PEN, ALLOC.PEN
	$(COMPILE) MONSTER_REBUILD
GLOBAL.OBJ + GLOBAL.PEN : GLOBAL.PAS
	$(COMPILE) GLOBAL		! Globaalit määritykset
DATABASE.OBJ + DATABASE.PEN : DATABASE.PAS,GLOBAL.PEN,GUTS.PEN
	$(COMPILE) DATABASE		! Tietokannan käsittely
MON.OBJ : MON.PAS,GLOBAL.PEN,DATABASE.PEN,GUTS.PEN,CLI.PEN, -
	PRIVUSERS.PEN,PARSER.PEN,INTERPRETER.PEN, QUEUE.PEN, CUSTOM.PEN -
	ALLOC.PEN
	$(COMPILE) MON			! MON.PASin käännös
GUTS.OBJ + GUTS.PEN : GUTS.PAS,GLOBAL.PEN
	$(COMPILE) GUTS			! GUTS.PASin käännös
INTERPRETER.OBJ + INTERPRETER.PEN : INTERPRETER.PAS,GLOBAL.PEN, -
DATABASE.PEN,PARSER.PEN
	$(COMPILE) INTERPRETER 		! INTERPRETER.PASin käännös
KEYS.OBJ + KEYS.PEN : KEYS.PAS,GLOBAL.PEN
	$(COMPILE)  KEYS		! Koodaustaulun käännös
PRIVUSERS.OBJ + PRIVUSERS.PEN : PRIVUSERS.PAS,GLOBAL.PEN
	$(COMPILE)  PRIVUSERS
QUEUE.OBJ + QUEUE.PEN : QUEUE.PAS,GLOBAL.PEN
	$(COMPILE) QUEUE 		! QUEUE.PASin käännös
PARSER.OBJ + PARSER.PEN : PARSER.PAS,GLOBAL.PEN,DATABASE.PEN,GUTS.PEN
	$(COMPILE) PARSER 		! PARSER.PASin käännös
CLI.OBJ + CLI.PEN : CLI.PAS,GLOBAL.PEN,DATABASE.PEN
	$(COMPILE) CLI
CUSTOM.OBJ + CUSTOM.PEN : CUSTOM.PAS, GLOBAL.PEN,DATABASE.PEN,GUTS.PEN, -
	CLI.PEN, ALLOC.PEN, PRIVUSERS.PEN,PARSER.PEN,INTERPRETER.PEN, QUEUE.PEN
	$(COMPILE) CUSTOM		! CUSTOM.PASin käännös
ALLOC.OBJ + ALLOC.PEN : ALLOC.PAS, DATABASE.PEN, GUTS.PEN, GLOBAL.PEN, -
	PRIVUSERS.PEN, PARSER.PEN
	$(COMPILE) ALLOC		! ALLOC.PASin käännös
VERSION.OBJ : VERSION.PAS, GLOBAL.PEN
	$(COMPILE) VERSION		! Monsterin versionumero
.LAST :
	show process/accounting

